#include<iostream>
using namespace std;
class arr{
	private:
	int m[5];
	public:
		void input();
		void output();
		void max();
		void min();
};
void arr::input()
{
	for(int i=0;i<5;i++)
	{
		cout<<"enter the values of array=";
		cin>>m[i];
	}
}
void arr::output()
{
	for(int i=0;i<5;i++)
	{
		cout<<m[i]<<endl;
	}
}
void arr::max()
{
	int mx=m[0];
	for(int i=0;i<5;i++)
	{
		if(mx<=m[i])
		{
			mx=m[i];
		}
	}
	cout<<"max="<<mx<<endl;
}
void arr::min()
{
	int mn=m[0];
	for(int i=0;i<5;i++)
	{
		if(mn>=m[i])
		{
			mn=m[i];
		}
	}
		cout<<"min="<<mn;

}
